{
    "env": {
        "browser": true,
        "es2021" : true,
        "node": true
    },
    "extends":["airbnb-base", "plugin:import/recommended", "plugin:cypress/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType" :"module"
    },
    "rules": {
        "import/extensions" : "off",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "import/prefer-default-export": "off",
        "no-alert": "off",
        "no-plusplus": "off",
        "consistent-return": "off"
        }
}